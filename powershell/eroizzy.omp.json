{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"transient_prompt": {
        "background": "transparent",
        "foreground": "#ffffff",
        "template": "\n{{ .Shell }}> "
    },
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#ff7b00",
					"foreground": "#000000",
					"leading_diamond": "\n\n\u256d\u2500\ue0b6",
					"trailing_diamond": "\ue0b4",
					"properties": {
						"template": "\uf489 {{.Shell}}"
					},
					"style": "diamond",
					"type": "shell"
				},				
				{
					"background": "#44007c",
					"foreground": "#00ff0d",
					"leading_diamond": " \ue0b6",
					"trailing_diamond": "\ue0b4",
					"properties": {
						"folder_icon": "<#B5B2C2>\ue5fe </>",
						"folder_separator_icon": " <#44007c,transparent>\ue0b0</><transparent,#44007c>\ue0b0 </> ",
						"home_icon": "\uf7db ",
						"style": "agnoster_full",
						"template": "<#ffffff> \ue5ff </>{{ .Path }}"
					  },
					"style": "diamond",
					"type": "path"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"background": "#303030",
					"foreground": "#3C873A",
					"leading_diamond": " \ue0b6",
					"properties": {
						"fetch_package_manager": true,
						"npm_icon": " <#cc3a3a>\ue5fa</> ",
						"template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
						"yarn_icon": " <#348cba>\uf61a</>"
					},
					"style": "diamond",
					"trailing_diamond": "\ue0b4",
					"type": "node"
				},
				{
					"background": "#ff5e00",
					"background_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
						"{{ if gt .Ahead 0 }}#C792EA{{ end }}",
						"{{ if gt .Behind 0 }}#C792EA{{ end }}"
					],
					"foreground": "#000000",
					"leading_diamond": " \ue0b6",
					"properties": {
						"branch_icon": "\ue725 ",
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"template": "{{ .HEAD }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046{{ .Staging.String }}</>{{ end }}"
					},
					"trailing_diamond": "\ue0b4",
					"style": "diamond",
					"type": "git"
				},
				{
					"background": "#ffffff",
					"foreground": "#000000",
					"invert_powerline": true,
					"leading_diamond": " \ue0b6",
					"properties": {
						"template": " \uf5ef {{ .CurrentDate | date .Format }} "
					},
					"style": "diamond",
					"trailing_diamond": "\ue0b4",
					"type": "time"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#ff7b00",
					"properties": {
						"template": "\u2502\n\u2570\u2500"
					},
					"style": "plain",
					"type": "text"
				},
				{
					"leading_diamond": "\ue0b2",
					"background": "#c90000",
					"foreground": "#ffffff",
					"properties": {
						"root_icon": "\ue22d",
						"template": "\ue22d"
					},
					"trailing_diamond": "\ue0b0",
					"style": "diamond",
					"type": "root"
				},
				{
					"leading_diamond": "\ue0b2",
					"background": "#3cff00",
					"foreground": "#9e00c5",
					"trailing_diamond": "\ue0b0",
					"style": "diamond",
					"type": "session"
					
				},
				{
					"foreground": "#e0f8ff",
					"foreground_templates": [
						"{{ if gt .Code 0 }}#ef5350{{ end }}"
					],
					"properties": {
						"always_enabled": true,
						"template": " \ue0b1 "
					},
					"style": "plain",
					"type": "exit"
				}
			],
			"type": "prompt"
		}
	],
	"osc99": true,
	"version": 1
}